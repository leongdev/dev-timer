{
  "expo": {
    "name": "Dev Timer", 
    "slug": "dev-timer",
    "scheme": "dev-timer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app_icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "packagerOpts": {
      "sourceExts": [
        "cjs"
      ]
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "2",
      "bundleIdentifier": "com.leongdev.devtimer",
      "supportsTablet": true,
      "usesAppleSignIn": true
    },
    "android": {
      "versionCode": 2,  
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.leongdev.devtimer"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
