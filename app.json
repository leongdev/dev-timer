{
  "expo": {
    "name": "Dev Timer",
    "slug": "dev-timer",
    "scheme": "dev-timer",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./src/assets/images/app_icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "packagerOpts": {
      "sourceExts": ["cjs"]
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "buildNumber": "7",
      "bundleIdentifier": "com.leongdev.devtimer",
      "supportsTablet": true,
      "usesAppleSignIn": true
    },
    "android": {
      "versionCode": 7,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.leongdev.devtimer"
    },
    "web": {
      "favicon": "./src/assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "901244c7-1786-418e-9979-b852e46986f0"
      }
    }
  }
}
